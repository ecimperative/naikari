#.
#. <?xml version='1.0' encoding='utf8'?>
#. <mission name="The Space Family">
#. <flags>
#. <unique />
#. </flags>
#. <avail>
#. <priority>4</priority>
#. <chance>100</chance>
#. <location>None</location>
#. </avail>
#. <notes>
